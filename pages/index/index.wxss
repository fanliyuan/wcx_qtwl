/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}
view{
  box-sizing:border-box;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}
.imgSize{
  width:750rpx;
}
.small{
  font-size:12px;
}
.banner{
  height:360rpx;
}
.banner navigator{
  height:100%;
}
.banner .banner_image{
  width:100%;
}
.search-block{
  background:#fff;
  padding:20rpx 0;
}
.search-block .address{
  font-size:13px;
  margin-left:13rpx;
  margin-right:20rpx;
}
.search-box{
  background:#F4F9FB;
  width:450rpx;
  margin-left:0;
  display:inline-flex;
 
  font-size:12px;
  align-items:center;
  height:65rpx;
  padding:0 35rpx;
  color:#000;
  border-radius:32.5rpx;
  vertical-align:middle;
}
#infos{
  display:inline-block;
  margin-left:20rpx;
}
#infos image{
  width:45rpx;
  height:45rpx;
  vertical-align:middle;
}
.search-box navigator{
  display:inline-flex;
  align-items:center;
}
.search-box icon{
  padding-right:10rpx;
}

.jingxue_list{
  
  display:block;
  font-size:0;
  width:1500rpx;
  padding:0 0 24rpx 24rpx;
}
.jingxue_list navigator{
  display:inline-block;
  font-size:0;
  background:#fff;
   margin-top:24rpx;
}
.jingxue_list navigator text{
  height:20px;
  width:120rpx;
  display:block;
  font-size:11px;
  text-align:center;
}
.jingxue_list navigator image{
  height:120rpx;
  width:131rpx;
 
}
.jingxue_list navigator + navigator{
  margin-left:16rpx;
 
}
.jingxue_list navigator:nth-child(11){
  margin-left:0;
}
.jingxue_list navigator:nth-child(6),.jingxue_list navigator:nth-child(16){
  margin-left:32rpx;
}
.merchantLsit{
  display:inline-block;
  width:375rpx;
  text-align:center;
  border-top:1px solid #eee;
  border-bottom:1px solid #eee;
  font-size:14px;
  padding:10rpx 0;
}
.merchantLsit.active{
  border-bottom-color:#e4393c;
  border-bottom-width:2px;
}
.merchant_shop,.shop_list{
  padding:20rpx 25rpx;
  margin-bottom:10rpx;
  background:#fff;
}
.shop_list{
   padding:20rpx 15rpx;
}
/* .merchant_shop>image,.shop_list>image{
  height:200rpx;
  width:200rpx;
  vertical-align:top;
} */
.merchant_shop .merchant_title,.shop_list .shop_title{
  display:inline-flex;
  width:300rpx;
  height:200rpx;
  margin-left:25rpx;
  justify-content:space-between;
  flex-direction:column;
}
.shop_list .shop_title{
  margin-left:15rpx;
}
.shop_list .shop_title{
  width:405rpx;
}
.merchant_shop .merchant_title .start{
  height:38rpx;
  width:220rpx;
}
.merchant_shop .merchant_name,.shop_list .shop_name{
  font-size:14px;
  color:#555;
}
.merchant_shop .merchant_ontime,.merchant_shop .merchant_classifies,.shop_list .shop_info{
  font-size:12px;
  color:#999;
}
.merchant_shop .distance{
  display:inline-block;
  width:155rpx;
  vertical-align:top;
}
.merchant_shop .distance text{
  font-size:12px;
  color:green;
  float:right;
}
#merchant_shop_list{
  background:#eee;
}
.shop_list .shop_onsale{
  font-size:12px;
  color:green;
}
#shop_list .isOn{
  width:100rpx;
  display:inline-block;
  vertical-align:top;
  font-size:11px;
  color:green;
  height:200rpx;
  position:relative;
  
}
#shop_list .isOn text{
  float:left;
}
#shop_list .isOn view{
  position:absolute;
  right:0;
  top:50%;
  transform:translate(0,-50%);
  color:#999;
  font-size:14px;
}
#shop_list .shop_price{
  font-size:12px;
}
#shop_list .shop_price .now_price{
  font-size:14px;
  color:#e4393c;
}
#shop_list .shop_price .old_price{
  color:#999;
  text-decoration:line-through;
}
#merchant_shop_list .forMore{
  padding:20rpx 0;
  background:#fff;
  font-size:12px;
  text-align:center;
  color:#555;
}
.mainList_index{
  margin-bottom:30rpx;
  position:relative;
}
#mainList_index{
  margin:-20rpx 20rpx 0;
  padding:30rpx 25rpx;
  background:#fff;
 box-shadow:1px 3px 3px #EEDFE1;
  width:710rpx;
  border-radius:5px;
  
}
#mainList_index image{
  width:100rpx;
  height:100rpx;
  margin:30rpx 0 20rpx;
}
#mainList_index navigator{
  box-sizing:border-box;
  display:inline-flex;
  flex-direction:column;
  width:132rpx;
  align-items:center;
}
#mainList_index text{
  font-size:30rpx;
  color:#313131;
}
#hotNew{
  margin-bottom:30rpx;
  position:relative;
}
#hotNew .content{
  position:absolute;
  left:20rpx;
  top:50%;
  transform:translate(0,-50%);
}
#hotNew .content .he{
  width:70rpx;
  vertical-align:middle;
}
#hotNew .hot_image{
  width:750rpx;

}
 #hotNew .content_font{
  width:80rpx;
  display:inline-block;
  vertical-align:middle;
  overflow:hidden;
  margin:0 20rpx;
} 
#hotNew .content_font image{
  width:80rpx;
  float:left;
} 
#hotNew .content_font image+image{
  margin-top:10rpx;
}
#hotNew .content_text{
  display:inline-block;
  vertical-align:middle;
}
#hotNew .content_text text{
  font-size:14px;
  display:block;
}
#activities{
  background:#fff;
  margin:50rpx 0;

}
#activities .sonsa{
  display:inline-block;

  height:465rpx;
  box-sizing:border-box;
  vertical-align:middle;
}
#activities .sonsa .sonss{
  border-bottom:2px solid #F4F4F4;
  border-right:2px solid #F4F4F4;
  width:250rpx;

}
#activities .sonsa .sonss image.goods{
  width:150rpx;
  height:140rpx;
  margin-left:20rpx;
}
#activities image.title{
  height:60rpx;

  width:60rpx;
  margin-left:20rpx;
}
#activities image.title.asp{
  width:120rpx;
}
#activities .info{
  height:30rpx;
  font-size:30rpx;
  line-height:30rpx;
  color:#929292;
  margin-left:20rpx;
  overflow:hidden;
}
image.dago{
  width:200rpx;
  height:80rpx;

  position:absolute;
  top:-40rpx;
  left:0;
}
#activities .icon{
  width:60rpx;
  height:60rpx;
  position:relative;
  left:-30rpx;
  top:-20rpx;
}
#tjShop{
  background:#fff;
  padding:40rpx 0; 
  margin-bottom:35rpx;
}
#scroll2{
  width:1500rpx;
}
#scroll2 image{
  width:100rpx;
  height:100rpx;
  margin-right:25rpx;
}
#scroll2 navigator{
  display:inline-flex;
  align-items:center;
  border-radius:8px;
  border:3px solid #F5F5F5;
  box-shadow:0 0 20px #F5F5F5;
  padding:25rpx;
}
#scroll2 navigator + navigator{
  margin-left:25rpx;
}
#scroll2 text{
  display:block;
  font-size:14px;
  color:#909090;
}

#inters .info{
  display:block;
  padding:15rpx;
  font-size:16px;
  text-overflow:ellipsis;
}
#inters .price{
  display:block;
  padding:15rpx;
  padding-top:0;
  font-size:14px;
  color:#e4393c;
  font-weight:bold;
  overflow:hidden;
}
#inters .price text{
  float:right;
  font-size:12px;
  font-weight:normal;
  color:#A9A9A9;

}
#inters navigator{
  width:370rpx;
  display:inline-block;
  vertical-align:middle;
  background:#fff;
  margin-bottom:10rpx;
}
#inters navigator.spe{
  margin-left:10rpx;
}
#inters navigator image{
  width:370rpx;
}
